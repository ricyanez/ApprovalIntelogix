@model IEnumerable<ApprovalIntelogix.Models.UserStep>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdStepNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pk)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdStepNavigation.IdStep)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pk.PkId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUserStep">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdUserStep">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUserStep">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
