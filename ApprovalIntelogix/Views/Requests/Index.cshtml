                    <a asp-action="Edit" asp-route-id="@item.IdRequest">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdRequest">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdRequest">Delete</a>
